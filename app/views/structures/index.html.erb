<h1>Listing structures</h1>

<table>
  <tr>
  	<th>Group</th>
  	<th>Entity</th>
  	<th>Division</th>
  	<th>Description</th>
  </tr>

<% for structure in @structures %>
  <tr>
  	<td><%=h structure.Group_Identifier %></td>
    <td><%=h structure.Entity_Identifier %></td>
    <td><%=h structure.Division_Identifier %></td>
    <td><%=h structure.Division_Description %></td>
    <td><%= link_to 'Show', structure %></td>
    <td><%= link_to 'Edit', edit_structure_path(structure) %></td>
    <td><%= link_to 'Destroy', structure, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New structure', new_structure_path %>
