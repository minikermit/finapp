<span class="myapptitle">Listing users</span>

<table>
	<tr>
		<th>Login</th>
		<th>E-Mail</th>
		<th>created at</th>
	</tr>

<% for user in @users %>
 <tr>
 	<td> <%=h user.login %> </td>
 	<td> <%=h user.email %> </td>
 	<td> <%=h user.created_at %> </td>
 </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>
