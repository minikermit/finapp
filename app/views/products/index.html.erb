<h1>Listing products</h1>

<table>
  <tr>
  </tr>

<% for product in @products %>
  <tr>
  	
  	<td><%=h product.FiMIS_Identifier %></td>
    <td><%=h product.DIM_Aggregate %></td>
    <td><%=h product.Product_Type %></td>
    <td><%=h product.Product_type_name %></td>
    <td><%=h product.Product_Kind %></td>
    <td><%=h product.Product_Kind_Name %></td>
    <td><%=h product.Product_GroupBy_1 %></td>
    <td><%=h product.Product_GroupBy_1_name %></td>
    <td><%=h product.Product_GroupBy_2 %></td>
    <td><%=h product.Product_GroupBy_2_name %></td>
    <td><%=h product.Product_GroupBy_3 %></td>
    <td><%=h product.Product_GroupBy_3_name %></td>
    <td><%=h product.Record_Creation_Date %></td>
    <td><%=h product.Record_creator %></td>
  	
    <td><%= link_to 'Show', product %></td>
    <td><%= link_to 'Edit', edit_product_path(product) %></td>
    <td><%= link_to 'Destroy', product, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product', new_product_path %>
