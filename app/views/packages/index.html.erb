
        


<span class="myapptitle">Listing Packages</span>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% for package in @packages %>
      <tr> 	
  	<td><%=h package.Source_Package_Name %></td>
    <td><%=h package.Source_Package_Description %></td>
    <td><%= link_to 'Show', package_path(1) %></td>


   </tr>
<% end %>
</table>

<br />

<%= link_to 'New package', new_package_path %>

