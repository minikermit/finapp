<h1>Editing report_category</h1>

<%= error_messages_for :report_category %>

<% form_for(@report_category) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :Category_Type_Description %>
  </p>
  <p>
    <b>Type</b><br />
    <%= f.text_field :Category_Kind_Description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @report_category %> |
<%= link_to 'Back', report_categories_path %>
